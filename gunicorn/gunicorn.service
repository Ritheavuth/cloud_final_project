[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/studygroup_project
ExecStart=/home/ubuntu/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/studygroup_project/studygroup/studygroup.sock studygroup.wsgi:application

       
[Install]
WantedBy=multi-user.target